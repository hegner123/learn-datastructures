{
  "testCases": [
    {
      "name": "New empty queue",
      "operations": [
        {"method": "isEmpty", "expected": true},
        {"method": "size", "expected": 0}
      ]
    },
    {
      "name": "Enqueue single item",
      "operations": [
        {"method": "enqueue", "args": ["hello"]},
        {"method": "isEmpty", "expected": false},
        {"method": "size", "expected": 1},
        {"method": "front", "expected": "hello"}
      ]
    },
    {
      "name": "Enqueue multiple items",
      "operations": [
        {"method": "enqueue", "args": [1]},
        {"method": "enqueue", "args": [2]},
        {"method": "enqueue", "args": [3]},
        {"method": "size", "expected": 3},
        {"method": "front", "expected": 1}
      ]
    },
    {
      "name": "Dequeue from non-empty queue",
      "operations": [
        {"method": "enqueue", "args": ["first"]},
        {"method": "enqueue", "args": ["second"]},
        {"method": "dequeue", "expected": "first"},
        {"method": "size", "expected": 1},
        {"method": "front", "expected": "second"}
      ]
    },
    {
      "name": "Dequeue from empty queue",
      "operations": [
        {"method": "dequeue", "expected": null}
      ]
    },
    {
      "name": "Front on empty queue",
      "operations": [
        {"method": "front", "expected": null}
      ]
    },
    {
      "name": "IsEmpty on empty queue",
      "operations": [
        {"method": "isEmpty", "expected": true}
      ]
    },
    {
      "name": "IsEmpty on non-empty queue",
      "operations": [
        {"method": "enqueue", "args": ["item"]},
        {"method": "isEmpty", "expected": false}
      ]
    },
    {
      "name": "Size tracking",
      "operations": [
        {"method": "size", "expected": 0},
        {"method": "enqueue", "args": [1]},
        {"method": "size", "expected": 1},
        {"method": "enqueue", "args": [2]},
        {"method": "size", "expected": 2},
        {"method": "dequeue", "expected": 1},
        {"method": "size", "expected": 1},
        {"method": "dequeue", "expected": 2},
        {"method": "size", "expected": 0}
      ]
    },
    {
      "name": "FIFO behavior",
      "operations": [
        {"method": "enqueue", "args": ["first"]},
        {"method": "enqueue", "args": ["second"]},
        {"method": "enqueue", "args": ["third"]},
        {"method": "enqueue", "args": ["fourth"]},
        {"method": "dequeue", "expected": "first"},
        {"method": "dequeue", "expected": "second"},
        {"method": "dequeue", "expected": "third"},
        {"method": "dequeue", "expected": "fourth"},
        {"method": "isEmpty", "expected": true}
      ]
    }
  ]
}