{
  "testCases": [
    {
      "name": "Basic merge",
      "arr1": [1, 3, 5],
      "arr2": [2, 4, 6],
      "expected": [1, 2, 3, 4, 5, 6]
    },
    {
      "name": "Empty first array",
      "arr1": [],
      "arr2": [1, 2, 3],
      "expected": [1, 2, 3]
    },
    {
      "name": "Empty second array",
      "arr1": [1, 2, 3],
      "arr2": [],
      "expected": [1, 2, 3]
    },
    {
      "name": "Both arrays empty",
      "arr1": [],
      "arr2": [],
      "expected": []
    },
    {
      "name": "Different lengths",
      "arr1": [1, 5, 9, 10, 15, 20],
      "arr2": [2, 3, 8, 13],
      "expected": [1, 2, 3, 5, 8, 9, 10, 13, 15, 20]
    },
    {
      "name": "First array larger elements",
      "arr1": [4, 5, 6],
      "arr2": [1, 2, 3],
      "expected": [1, 2, 3, 4, 5, 6]
    },
    {
      "name": "Duplicate elements",
      "arr1": [1, 3, 5],
      "arr2": [1, 3, 5],
      "expected": [1, 1, 3, 3, 5, 5]
    },
    {
      "name": "Negative numbers",
      "arr1": [-5, -1, 2],
      "arr2": [-3, 0, 4],
      "expected": [-5, -3, -1, 0, 2, 4]
    },
    {
      "name": "Single elements",
      "arr1": [5],
      "arr2": [3],
      "expected": [3, 5]
    },
    {
      "name": "Large ranges",
      "arr1": [100, 200, 300],
      "arr2": [150, 250, 350, 400, 500],
      "expected": [100, 150, 200, 250, 300, 350, 400, 500]
    }
  ]
}