{
  "testCases": [
    {
      "name": "Basic case",
      "input": {
        "nums": [1, 3, -1, -3, 5, 3, 6, 7],
        "k": 3
      },
      "expected": [3, 3, 5, 5, 6, 7]
    },
    {
      "name": "Single element",
      "input": {
        "nums": [1],
        "k": 1
      },
      "expected": [1]
    },
    {
      "name": "Window size equals array length",
      "input": {
        "nums": [1, -1, 0, 5, 4],
        "k": 5
      },
      "expected": [5]
    },
    {
      "name": "All same elements",
      "input": {
        "nums": [4, 4, 4, 4],
        "k": 2
      },
      "expected": [4, 4, 4]
    },
    {
      "name": "Decreasing array",
      "input": {
        "nums": [9, 8, 7, 6, 5],
        "k": 3
      },
      "expected": [9, 8, 7]
    },
    {
      "name": "Increasing array",
      "input": {
        "nums": [1, 2, 3, 4, 5],
        "k": 3
      },
      "expected": [3, 4, 5]
    },
    {
      "name": "Negative numbers",
      "input": {
        "nums": [-7, -8, 7, 5, 7, 1, 6, 0],
        "k": 4
      },
      "expected": [7, 7, 7, 7, 7]
    },
    {
      "name": "Large window size",
      "input": {
        "nums": [1, 2, 3, 4],
        "k": 3
      },
      "expected": [3, 4]
    },
    {
      "name": "Mixed positive negative",
      "input": {
        "nums": [-1, 2, -3, 4, -5, 6],
        "k": 2
      },
      "expected": [2, 2, 4, 4, 6]
    },
    {
      "name": "Peak in middle",
      "input": {
        "nums": [1, 3, 1, 2, 0, 5],
        "k": 3
      },
      "expected": [3, 3, 2, 5]
    }
  ]
}