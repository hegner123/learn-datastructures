{
  "testCases": [
    {
      "name": "Basic case",
      "input": {
        "nums": [2, 7, 11, 15],
        "target": 9
      },
      "expected": [0, 1]
    },
    {
      "name": "Reverse order", 
      "input": {
        "nums": [15, 11, 7, 2],
        "target": 9
      },
      "expected": [2, 3]
    },
    {
      "name": "Same elements",
      "input": {
        "nums": [3, 3],
        "target": 6
      },
      "expected": [0, 1]
    },
    {
      "name": "Negative numbers",
      "input": {
        "nums": [-1, -2, -3, -4, -5],
        "target": -8
      },
      "expected": [2, 4]
    },
    {
      "name": "Mixed signs",
      "input": {
        "nums": [-3, 4, 3, 90],
        "target": 0
      },
      "expected": [0, 2]
    },
    {
      "name": "Zero target",
      "input": {
        "nums": [-2, 0, 1, 1],
        "target": 2
      },
      "expected": [2, 3]
    },
    {
      "name": "Large numbers",
      "input": {
        "nums": [999, 1, 2],
        "target": 1000
      },
      "expected": [0, 1]
    },
    {
      "name": "Adjacent elements",
      "input": {
        "nums": [1, 2, 3, 4],
        "target": 7
      },
      "expected": [2, 3]
    },
    {
      "name": "First and last",
      "input": {
        "nums": [5, 1, 2, 8],
        "target": 13
      },
      "expected": [0, 3]
    },
    {
      "name": "Middle elements",
      "input": {
        "nums": [10, 20, 30, 40, 50],
        "target": 70
      },
      "expected": [2, 3]
    }
  ]
}
