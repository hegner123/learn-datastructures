{
  "testCases": [
    {
      "name": "Insert single value and search",
      "operations": [
        {"type": "insert", "value": 5},
        {"type": "search", "value": 5, "expected": true}
      ]
    },
    {
      "name": "Insert multiple values and search existing",
      "operations": [
        {"type": "insert", "value": 10},
        {"type": "insert", "value": 5},
        {"type": "insert", "value": 15},
        {"type": "search", "value": 5, "expected": true},
        {"type": "search", "value": 10, "expected": true},
        {"type": "search", "value": 15, "expected": true}
      ]
    },
    {
      "name": "Search for non-existing value",
      "operations": [
        {"type": "insert", "value": 10},
        {"type": "insert", "value": 20},
        {"type": "search", "value": 15, "expected": false}
      ]
    },
    {
      "name": "In-order traversal of simple tree",
      "operations": [
        {"type": "insert", "value": 10},
        {"type": "insert", "value": 5},
        {"type": "insert", "value": 15},
        {"type": "traversal", "expected": [5, 10, 15]}
      ]
    },
    {
      "name": "In-order traversal with more complex tree",
      "operations": [
        {"type": "insert", "value": 50},
        {"type": "insert", "value": 30},
        {"type": "insert", "value": 20},
        {"type": "insert", "value": 40},
        {"type": "insert", "value": 70},
        {"type": "traversal", "expected": [20, 30, 40, 50, 70]}
      ]
    },
    {
      "name": "Insert duplicate values",
      "operations": [
        {"type": "insert", "value": 10},
        {"type": "insert", "value": 10},
        {"type": "insert", "value": 10},
        {"type": "traversal", "expected": [10]}
      ]
    },
    {
      "name": "Left-skewed tree operations",
      "operations": [
        {"type": "insert", "value": 30},
        {"type": "insert", "value": 20},
        {"type": "insert", "value": 10},
        {"type": "search", "value": 20, "expected": true},
        {"type": "traversal", "expected": [10, 20, 30]}
      ]
    },
    {
      "name": "Right-skewed tree operations",
      "operations": [
        {"type": "insert", "value": 10},
        {"type": "insert", "value": 20},
        {"type": "insert", "value": 30},
        {"type": "search", "value": 25, "expected": false},
        {"type": "traversal", "expected": [10, 20, 30]}
      ]
    },
    {
      "name": "Empty tree operations",
      "operations": [
        {"type": "search", "value": 10, "expected": false},
        {"type": "traversal", "expected": []}
      ]
    },
    {
      "name": "Large set insertion and search",
      "operations": [
        {"type": "insert", "value": 50},
        {"type": "insert", "value": 25},
        {"type": "insert", "value": 75},
        {"type": "insert", "value": 12},
        {"type": "insert", "value": 37},
        {"type": "insert", "value": 62},
        {"type": "insert", "value": 87},
        {"type": "search", "value": 37, "expected": true},
        {"type": "search", "value": 100, "expected": false},
        {"type": "traversal", "expected": [12, 25, 37, 50, 62, 75, 87]}
      ]
    }
  ]
}