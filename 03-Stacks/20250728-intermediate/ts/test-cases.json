{
  "testCases": [
    {
      "id": 1,
      "name": "Basic MinStack Operations",
      "description": "Test basic push, getMin, peek, and size operations",
      "operations": [
        {"type": "push", "value": 3},
        {"type": "push", "value": 1},
        {"type": "push", "value": 4},
        {"type": "getMin", "expected": 1},
        {"type": "peek", "expected": 4},
        {"type": "size", "expected": 3}
      ]
    },
    {
      "id": 2,
      "name": "Pop and Min Update",
      "description": "Test pop operation and minimum value update",
      "operations": [
        {"type": "push", "value": 5},
        {"type": "push", "value": 2},
        {"type": "push", "value": 7},
        {"type": "pop", "expected": 7},
        {"type": "getMin", "expected": 2}
      ]
    },
    {
      "id": 3,
      "name": "Multiple Same Minimum Values",
      "description": "Test handling of multiple identical minimum values",
      "operations": [
        {"type": "push", "value": 2},
        {"type": "push", "value": 3},
        {"type": "push", "value": 2},
        {"type": "push", "value": 1},
        {"type": "push", "value": 2},
        {"type": "pop", "expected": 2},
        {"type": "getMin", "expected": 1}
      ]
    },
    {
      "id": 4,
      "name": "Basic Undo Push Operation",
      "description": "Test undoing a push operation",
      "operations": [
        {"type": "push", "value": 10},
        {"type": "push", "value": 5},
        {"type": "undo", "expected": true},
        {"type": "peek", "expected": 10},
        {"type": "size", "expected": 1}
      ]
    },
    {
      "id": 5,
      "name": "Basic Undo Pop Operation",
      "description": "Test undoing a pop operation",
      "operations": [
        {"type": "push", "value": 1},
        {"type": "push", "value": 2},
        {"type": "pop", "expected": 2},
        {"type": "undo", "expected": true},
        {"type": "peek", "expected": 2},
        {"type": "size", "expected": 2}
      ]
    },
    {
      "id": 6,
      "name": "Multiple Undo Operations",
      "description": "Test multiple consecutive undo operations",
      "operations": [
        {"type": "push", "value": 1},
        {"type": "push", "value": 2},
        {"type": "push", "value": 3},
        {"type": "undo", "expected": true},
        {"type": "undo", "expected": true},
        {"type": "peek", "expected": 1},
        {"type": "getMin", "expected": 1}
      ]
    },
    {
      "id": 7,
      "name": "Undo on Empty History",
      "description": "Test undo operation when no history exists",
      "operations": [
        {"type": "undo", "expected": false},
        {"type": "isEmpty", "expected": true}
      ]
    },
    {
      "id": 8,
      "name": "History Tracking",
      "description": "Test operation history tracking functionality",
      "operations": [
        {"type": "push", "value": 5},
        {"type": "push", "value": 3},
        {"type": "pop", "expected": 3},
        {"type": "push", "value": 7},
        {"type": "getHistory", "expected": ["Push(5)", "Push(3)", "Pop()", "Push(7)"]}
      ]
    },
    {
      "id": 9,
      "name": "Clear History Functionality",
      "description": "Test clearing history while preserving stack contents",
      "operations": [
        {"type": "push", "value": 1},
        {"type": "push", "value": 2},
        {"type": "pop", "expected": 2},
        {"type": "clearHistory"},
        {"type": "getHistory", "expected": []},
        {"type": "size", "expected": 1},
        {"type": "peek", "expected": 1}
      ]
    },
    {
      "id": 10,
      "name": "Complex Mixed Operations with Undo",
      "description": "Complex scenario with multiple operations and undos",
      "operations": [
        {"type": "push", "value": 4},
        {"type": "push", "value": 1},
        {"type": "push", "value": 6},
        {"type": "pop", "expected": 6},
        {"type": "getMin", "expected": 1},
        {"type": "undo", "expected": true},
        {"type": "getMin", "expected": 1},
        {"type": "pop", "expected": 6},
        {"type": "getMin", "expected": 1},
        {"type": "undo", "expected": true},
        {"type": "getMin", "expected": 1}
      ]
    }
  ]
}