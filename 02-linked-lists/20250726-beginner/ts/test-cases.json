{
  "testCases": [
    {
      "name": "Empty list",
      "input": [],
      "expected": []
    },
    {
      "name": "Single element",
      "input": [5],
      "expected": [5]
    },
    {
      "name": "Two elements",
      "input": [1, 2],
      "expected": [2, 1]
    },
    {
      "name": "Three elements",
      "input": [1, 2, 3],
      "expected": [3, 2, 1]
    },
    {
      "name": "Multiple elements",
      "input": [1, 2, 3, 4, 5],
      "expected": [5, 4, 3, 2, 1]
    },
    {
      "name": "Negative numbers",
      "input": [-1, -2, -3],
      "expected": [-3, -2, -1]
    },
    {
      "name": "Mixed positive and negative",
      "input": [-2, 0, 3, -1],
      "expected": [-1, 3, 0, -2]
    },
    {
      "name": "Duplicate elements",
      "input": [1, 2, 2, 1],
      "expected": [1, 2, 2, 1]
    },
    {
      "name": "All same elements",
      "input": [7, 7, 7, 7],
      "expected": [7, 7, 7, 7]
    },
    {
      "name": "Large numbers",
      "input": [1000000, 2000000, 3000000],
      "expected": [3000000, 2000000, 1000000]
    }
  ]
}