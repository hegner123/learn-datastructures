{
  "testCases": [
    {
      "name": "Both lists empty",
      "input": {
        "list1": [],
        "list2": []
      },
      "expected": []
    },
    {
      "name": "First list empty",
      "input": {
        "list1": [],
        "list2": [1, 3, 5]
      },
      "expected": [1, 3, 5]
    },
    {
      "name": "Second list empty",
      "input": {
        "list1": [2, 4, 6],
        "list2": []
      },
      "expected": [2, 4, 6]
    },
    {
      "name": "Simple merge",
      "input": {
        "list1": [1, 3, 5],
        "list2": [2, 4, 6]
      },
      "expected": [1, 2, 3, 4, 5, 6]
    },
    {
      "name": "Lists with different lengths",
      "input": {
        "list1": [1, 5, 9, 15],
        "list2": [2, 3]
      },
      "expected": [1, 2, 3, 5, 9, 15]
    },
    {
      "name": "All elements from first list come first",
      "input": {
        "list1": [1, 2, 3, 4],
        "list2": [5, 6, 7, 8]
      },
      "expected": [1, 2, 3, 4, 5, 6, 7, 8]
    },
    {
      "name": "All elements from second list come first",
      "input": {
        "list1": [10, 20, 30],
        "list2": [1, 2, 3, 4, 5]
      },
      "expected": [1, 2, 3, 4, 5, 10, 20, 30]
    },
    {
      "name": "Duplicate elements",
      "input": {
        "list1": [1, 3, 5, 5],
        "list2": [2, 3, 4, 5]
      },
      "expected": [1, 2, 3, 3, 4, 5, 5, 5]
    },
    {
      "name": "Single element lists",
      "input": {
        "list1": [5],
        "list2": [3]
      },
      "expected": [3, 5]
    },
    {
      "name": "Negative numbers",
      "input": {
        "list1": [-5, -1, 3],
        "list2": [-3, 0, 2, 4]
      },
      "expected": [-5, -3, -1, 0, 2, 3, 4]
    }
  ]
}